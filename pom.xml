    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <java.version>11</java.version>

        <spring-cloud.version>Greenwich.SR2</spring-cloud.version>
        <hibernate.version>5.4.3.Final</hibernate.version>
        <hikari.version>3.3.1</hikari.version>
        <springfox.version>2.9.2</springfox.version>
        <xz.version>1.8</xz.version>
        <mysql.version>8.0.16</mysql.version>
        <slf4j.version>1.7.26</slf4j.version>
        <aspectj.version>1.9.4</aspectj.version>
        <swagger.version>1.5.20</swagger.version>
        <commons-crypto.version>1.0.0</commons-crypto.version>
        <commons-logging.version>1.2</commons-logging.version>
        <hibernate-validator.version>6.0.17.Final</hibernate-validator.version>

        <v2.aws.version>2.5.66</v2.aws.version>
        <aws.version>1.11.575</aws.version>
        <ay-core.version>4.4.2</ay-core.version>
        <ay-oss.version>3.4.2</ay-oss.version>
        <ay-sms.version>1.1.0</ay-sms.version>
        <ay-mns.version>1.1.8.6</ay-mns.version>
        <ay-dm.version>3.3.1</ay-dm.version>
        <ay-sts.version>3.0.1</ay-sts.version>
        <ay-log.version>0.1.16</ay-log.version>
        <ay-payed.version>3.7.89.ALL</ay-payed.version>

        <gaiamount.version>${project.version}</gaiamount.version>
        <docker.image.prefix>192.168.1.118:5000/gm</docker.image.prefix>
        <docker.image.base>${docker.image.prefix}/serverjre:11.0.3.1</docker.image.base>
    </properties>

