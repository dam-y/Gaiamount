    <groupId>com.cxria</groupId>
    <artifactId>gaiamount</artifactId>
    <version>1.0.0</version>
    <packaging>pom</packaging>

    <name>Gaiamount</name>
    <description>项目运行时依赖</description>
    <url>https://works.gaiamount.com</url>
    <developers>
        <developer>
            <name>杨欣武</name>
            <organization>cxria</organization>
        </developer>
    </developers>
    <scm>
        <url>https://github.com/xinwu-yang/Gaiamount.git</url>
